# Uncomment the next line to define a global platform for your project
platform :ios, '8.0'
target 'Practice' do
use_frameworks!
pod 'SwiftMessages'#Tost(Swift)
pod 'Alamofire'#网络请求   (Swift)
pod 'SDWebImage'   #图片缓存(网络图片请求拓展类 (oc)
pod 'MJRefresh'             #上啦下啦刷新库 (oc)
pod 'IQKeyboardManager'                     #键盘管理 (oc)
pod 'BaiduMapKit' #百度地图API
pod 'TZImagePickerController' #多选照片
pod 'Hero' #转场动画
pod 'NVActivityIndicatorView' #加载指示器
pod 'SnapKit' #Swift约束
pod 'PermissionScope' #权限调用
pod 'EZSwiftExtensions' #Swift Foundation和UIKit的有用扩展的集合
end
